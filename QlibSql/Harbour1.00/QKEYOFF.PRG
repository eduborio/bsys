function qkeyoff
set key 28 to // QHELP          // F1
set key -1 to // QSELECT_EMP    // F2
set key -2 to // QLOGIN         // F3
set key -3 to // QMENU          // F4
set key -4 to // QTOOLS         // F5
set key -5 to // QCALC          // F6
set key -6 to // QAGENDA        // F7
set key -7 to // QMEMOUSR       // F8
set key -9 to // QQUIT          // F10

return

