/////////////////////////////////////////////////////////////////////////////
// CRITICA ADICIONAL NA DESCIDA 1 ___________________________________________

static function i_critica1 ( cCAMPO )
   qmensa()
   do case
      case cCAMPO == "MUDOU_INSC"
           qrsay(XNIVEL , qabrev(fMUDOU_INSC,"SN",{"Sim","N„o"}))
           if fMUDOU_INSC == "N"
              qsay(12,44,space(27))
              qsay(14,29,space(14))
              fCAUSA_ALTE := space(1)
              fINSC_ANTE  := space(14)
              XNIVEL+=2
           endif
      case cCAMPO == "CAUSA_ALTE"
           if fMUDOU_INSC == "S"
              qrsay(XNIVEL , qabrev(fCAUSA_ALTE,"123456",{"Fus„o","Incorpora‡„o","Cis„o","Mudan‡a de CEI para CGC","Encerramento das Atividades","CEI vinculada ao CGC"}))
           endif
      case cCAMPO == "MUDOU_END"
           qrsay(XNIVEL , qabrev(fMUDOU_END,"SN",{"Sim","N„o"}))
      case cCAMPO == "MES_DTBASE"
           if empty(qtiraponto(fMES_DTBASE))
              qmensa("Campo MES DA DATA BASE ‚ obrigat¢rio !","B")
              return .F.
           endif
      case cCAMPO == "PROPRIET"
           qrsay(XNIVEL , fPROPRIET := strzero(val(fPROPRIET),2))
      case cCAMPO == "NATU_JURID"
           if empty(fNATU_JURID)
              qmensa("Campo NATUREZA JURIDICA ‚ obrigat¢rio !  Pressione <F9>","B")
              return .F.
           endif
           if ! NAT_JUR->(dbseek(fNATU_JURID))
              qmensa("Campo NATUREZA JURIDICA nÆo encontrado!  Pressione <F9>","B")
              return .F.
           Endif
      case cCAMPO == "TRAIS"
           qrsay(XNIVEL , qabrev(fTRAIS,"01",{"Com empregados","Sem empregados"}))

      case cCAMPO == "PART_PAT"
           qrsay(XNIVEL , qabrev(fPART_PAT,"SN",{"Sim","N„o"}))

      case cCAMPO == "MICRO_EMP"
           qrsay(XNIVEL , qabrev(fMICRO_EMP,"SN",{"Sim","N„o"}))

      case cCAMPO == "PEQ_PORTE"
           qrsay(XNIVEL , qabrev(fPEQ_PORTE,"SN",{"Sim","N„o"}))

      case cCAMPO == "OP_SIMPLES"
           qrsay(XNIVEL , qabrev(fOP_SIMPLES,"SN",{"Sim","N„o"}))

   endcase

return .T.

