                nHOR_03   := 0
                nHOR_04   := 0
                nHOR_05   := 0
                nHOR_06   := 0
                nHOR_07   := 0
                nHOR_08   := 0
                nHOR_09   := 0
                nHOR_10   := 0
                nHOR_11   := 0
                nHOR_12   := 0

                nFERIAS_IND := 0



                cRAIS += "2"
                cRAIS += "MATR. "+FUN->Matricula


                RAIS->(qappend())
                RAIS->Linha := cRAIS
                cRAIS := ""

                nTOTFUN++

                FUN->(dbskip())

             enddo



          cCGCCEI := FILIAL->Cgccpf

          FILIAL->(dbskip())

       enddo

       qmensa()

       CONFIG->(dbclosearea())

       AFAST->(dbclosearea())

       FUN->(dbclosearea())

       LANC->(dbclosearea())

       FILIAL->(dbclosearea())

       SITUA->(dbclosearea())

       BASE->(dbclosearea())

       CFGDISQ->(dbskip())

   enddo

   // MONTA REGISTRO TIPO 9 _________________________________________________

   nSEQ++

   cRAIS := strzero(nSEQ,6) + cCGCCEI + "009"
   cRAIS += strzero(nTOTEMP,6) + strzero(nTOTFUN,6)
   cRAIS += replicate(space(1),245)

   RAIS->(qappend())
   RAIS->Linha := cRAIS
   cRAIS := ""

   // GRAVA ARQUIVO NO DISQUETE _____________________________________________

   qmensa("Aguarde...Gravando arquivo...","B")

