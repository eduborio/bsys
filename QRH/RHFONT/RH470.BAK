                               if cMOT3 == "00" .and. dAFINI1 != strzero(day(SITUA->Af_ini),2)+strzero(Month(SITUA->Af_ini),2) .and. dAFINI2 != strzero(day(SITUA->Af_ini),2)+strzero(Month(SITUA->Af_ini),2)
                                  cMOT3 := "40"
                                  dAFINI3 := strzero(day(SITUA->Af_ini),2)+strzero(Month(SITUA->Af_ini),2)
                                  dAFFIM3 := strzero(day(SITUA->Af_ret),2)+strzero(Month(SITUA->Af_ret),2)
