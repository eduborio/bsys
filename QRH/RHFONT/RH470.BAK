                cRAIS += "00" //Competencias Outras Gratifica‡äes
                cRAIS += "00000000" //Valor Multa de RescisÆo sem Justa Causa
                cRAIS += "00000000000000"+"00000000" //Contribui‡Æo Associativa 1
                cRAIS += "00000000000000"+"00000000" //Contribui‡Æo Associativa 2

                if nCONTRIB_SIND > 0
                   SIND->(dbseek(FUN->Sind_cod))
                   cRAIS += iif(!empty(SIND->cnpj),left(qtiraponto(SIND->Cnpj),14),"00000000000000")
                   cRAIS += strzero(val(qtiraponto(str(nCONTRIB_SIND,8,2))),8)
                else
                   cRAIS += "00000000000000"+"00000000" //Sindical
                endif

                cRAIS += "00000000000000"+"00000000" //Contribui‡Æo Assistencial

                if nCONTRIB_CONF > 0
                   SIND->(dbseek(FUN->Sind_cod))
                   cRAIS += iif(!empty(SIND->cnpj),left(qtiraponto(SIND->Cnpj),14),"00000000000000")
                   cRAIS += strzero(val(qtiraponto(str(nCONTRIB_CONF,8,2))),8)
                else
                   cRAIS += "00000000000000"+"00000000" //Contribui‡Æo Confederativa
                endif
                cRAIS+=  CGM->Cod_rais

                cRAIS += strzero(nHOR_01,3)
                cRAIS += strzero(nHOR_02,3)
                cRAIS += strzero(nHOR_03,3)
                cRAIS += strzero(nHOR_04,3)
                cRAIS += strzero(nHOR_05,3)
                cRAIS += strzero(nHOR_06,3)
                cRAIS += strzero(nHOR_07,3)
                cRAIS += strzero(nHOR_08,3)
                cRAIS += strzero(nHOR_09,3)
                cRAIS += strzero(nHOR_10,3)
                cRAIS += strzero(nHOR_11,3)
                cRAIS += strzero(nHOR_12,3)


