   cNOMARQ := "C:\QSYSTXT\RAIS"+cANOBASE+".TXT"

   RAIS->(dbgotop())

   RAIS->(__dbSDF( .T., CNOMARQ , { } ,,,,, .F. ) )

   qmensa()

   select CFGDISQ

