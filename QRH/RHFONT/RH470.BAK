                dINI1 := ctod(left(dAFINI1,2)+"/"+right(dAFINI1,2)+"/"+cANOBASE)
                dFIM1 := ctod(left(dAFFIM1,2)+"/"+right(dAFFIM1,2)+"/"+cANOBASE)
                dINI2 := ctod(left(dAFINI2,2)+"/"+right(dAFINI2,2)+"/"+cANOBASE)
                dFIM2 := ctod(left(dAFFIM2,2)+"/"+right(dAFFIM2,2)+"/"+cANOBASE)
