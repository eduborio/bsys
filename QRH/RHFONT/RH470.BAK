                                  dAFINI2 := strzero(day(SITUA->Af_ini),2)+strzero(Month(SITUA->Af_ini),2)
                                  dAFFIM2 := strzero(day(SITUA->Af_ret),2)+strzero(Month(SITUA->Af_ret),2)
                               endif

