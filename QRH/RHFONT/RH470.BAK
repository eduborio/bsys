                   nDIAS += dFIM2 - dINI2
                endif

                if dAFINI3 != "0000" .and. dAFFIM3 != "0000"
                   nDIAS += dFIM3 - dINI3
                endif

                cRAIS += cMOT1+dAFINI1+dAFFIM1
                cRAIS += cMOT2+dAFINI2+dAFFIM2
                cRAIS += cMOT3+dAFINI3+dAFFIM3
                cRAIS += iif( nDIAS < 0,"000",strzero(nDIAS,3))
                cRAIS += iif(nFERIAS_IND > 0,strzero(val(qtiraponto(str(nFERIAS_IND,8,2))),8),"00000000")
